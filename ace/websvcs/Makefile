all: websvcs Websvcs_Test

clean:
	@cd lib
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f websvcs.bor clean
	@cd ../
	@cd tests
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f Websvcs_Test.bor clean
	@cd ../

realclean:
	@cd lib
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f websvcs.bor realclean
	@cd ../
	@cd tests
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f Websvcs_Test.bor realclean
	@cd ../

install:
	@cd lib
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f websvcs.bor install
	@cd ../
	@cd tests
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f Websvcs_Test.bor install
	@cd ../

websvcs:
	@cd lib
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f websvcs.bor all
	@cd ../

