	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f DLL_Newsweek.bor realclean
	$(MAKE) -$(MAKEFLAGS) $(MAKE_FLAGS) -f DLL_Main.bor realclean

