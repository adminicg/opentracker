# Makefile for building the Timer_Queue_Reference_Counting_Test exe with Borland C++ Make

NAME = Timer_Queue_Reference_Counting_Test

NO_FULL_PATH=1

OBJFILES = \
	$(OBJDIR)\Main.$(OBJ_EXT) \
	$(OBJDIR)\Timer_Queue_Reference_Counting_Test.$(OBJ_EXT)

LFLAGS = \
	-L"..\lib\$(CONFIG_SUBDIR)" \
	-j"..\lib\$(CONFIG_SUBDIR)" 

LIBFILES = \
	Test_Output$(LIB_DECORATOR).lib \
	ACE$(LIB_DECORATOR).lib \
	$(DUMMY_VALUE_NOT_ENDING_IN_BACKSLASH)

!ifdef STATIC
LIB_FLAGS = \
        -DACE_AS_STATIC_LIBS
!else
!endif

CFLAGS = \
	-I".." \
	$(LIB_FLAGS) \
	$(DLL_FLAGS)

CPPDIR = .;..\tests
CDIR   = .;..\tests

#
# Override defaults in outputdir.bor
#
STATIC_DIR=.
DEBUG_DIR=.
PASCAL_DIR=.
UNICODE_DIR=.

INSTALL_DLLDIR=$(INSTALL_DIR)\lib
INCLUDES_INSTALL=1

BASE_BINDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

includes_install:
	@echo Nothing to install.
