<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE OpenTracker SYSTEM "opentracker.dtd">
<OpenTracker>
  <configuration>
    <ConsoleConfig headerline="My Output" interval="1"/>
  </configuration>

  <ConsoleSink comment="Test Source 1">
    <EventTransform translation="1 1 1" scale="1 1 1"
    rotationtype="euler" rotation="0 0 0"> 
      
	<TestSource frequency="10.0" noise="0.1" />
    </EventTransform>
  </ConsoleSink>

  <ConsoleSink comment="Test Source 2">
    <EventTransform translation="1 1 1" scale="1 1 1"
    rotationtype="euler" rotation="0 0 0"> 
      
	<TestSource frequency="0.2" offset="500000" noise="0.1" />
    </EventTransform>
  </ConsoleSink>

  <ConsoleSink comment="Test Source 3">
    <EventTransform translation="1 1 1" scale="1 1 1"
    rotationtype="euler" rotation="0 0 0"> 
      
	<TestSource frequency="0.1" noise="0.15" />
    </EventTransform>
  </ConsoleSink>

</OpenTracker>
