    <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
    <!-- Camera Tracking -->
    <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
    <!-- Intertrax oriented to typical OpenInventor camera space -->
    <EventVirtualOrientationTransform rotation="0 -1 0 0" DEF="intertrax">
	    <EventOrientationTransform rotation="0 1 0 0">
		    <EventVirtualOrientationTransform rotationtype="matrix" rotation="0 0 1 -1 0 0 0 -1 0" >
			    <EventOrientationTransform rotation="0 -1 0 0 0 -1 1 0 0" rotationtype="matrix">
				    <InterSenseSource id="InterTrax"/>
			    </EventOrientationTransform>
		    </EventVirtualOrientationTransform>
		</EventOrientationTransform>
	</EventVirtualOrientationTransform>
	<!-- Offsets the readings from InterTrax by it's inverse everytime at least ONE marker is visible. we use allLocal to provide that -->
	<EventDynamicTransform DEF="head_offset">
      <TransformBase>
        <EventInvertTransform>
          <TimeGate timeframe="100">
            <Gate>
              <Ref USE="allLocal"/>
            </Gate>
            <Ref USE="intertrax"/>
          </TimeGate>
        </EventInvertTransform>
      </TransformBase>
      <Ref USE="intertrax"/>
    </EventDynamicTransform>
    <!-- output to camera station -->
    <StbSink station="20" DEF="acamera">
        <EventVirtualTransform DEF="acalibOffsetCam">
            <Ref USE="head_offset" />
            <!-- <TestSource frequency="10" position="0 0 0"/> -->
        </EventVirtualTransform>
    </StbSink>
