  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

  <!-- Camera Tracking -->

  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

  <!-- All local events merged (for inertia tracking resets) -->

  <!-- Intertrax oriented to typical OpenInventor camera space -->
  
  <EventVirtualOrientationTransform DEF="orientation" rotation=" 1 0  0 
    					                         0 0 -1
    					                         0 1  0 " rotationtype="matrix">
    <EventOrientationTransform rotation="1  0 0
  				  	 0  0 1 
  					 0 -1 0" rotationtype="matrix">
      <XSensSource comport="1"/>
    </EventOrientationTransform>
  </EventVirtualOrientationTransform>

  <!-- Offsets the readings from inertia tracker by it's inverse everytime at least ONE marker is visible. we use allLocal to provide that -->

  <EventDynamicTransform DEF="head_offset">
    <TransformBase>
      <EventInvertTransform>
        <TimeGate timeframe="100">
          <Gate>
            <ConfidenceFilter treshhold="0.5" type="high">
              <Ref USE="AllMarkers"/>
            </ConfidenceFilter>
          </Gate>
          <Ref USE="orientation"/>
        </TimeGate>
      </EventInvertTransform>
    </TransformBase>
    <Ref USE="orientation"/>
  </EventDynamicTransform>

  <!-- output to camera station -->

  <!-- output to camera station -->

  <Merge DEF="Head">
    <MergePosition>
      <Filter weight="0.25 0.25 0.25 0.25">
        <EventQueue length="4">
          <Ref USE="RawTracking"/>
        </EventQueue>
      </Filter>
    </MergePosition>
    <MergeOrientation>
      <EventDynamicTransform baseevent="false">
        <TransformBase>
          <ConfidenceFilter treshhold="0.5" type="high">
            <Ref USE="RawTracking"/>
          </ConfidenceFilter>
        </TransformBase>
        <Ref USE="head_offset"/>
      </EventDynamicTransform>
    </MergeOrientation>
  </Merge>

  <StbSink DEF="camera" station="2">
    <EventVirtualTransform DEF="calibOffsetCam" rotation="-0.001828 -0.000715 -0.007485 0.999970" translation="-0.033293 0.053453 -0.0379999">
      <Ref USE="Head"/>
    </EventVirtualTransform>
  </StbSink>

  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

  <!-- Devices -->

  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

  <!-- PIP -->

  <StbSink DEF="pip" station="1">
    <EventTransform translation="0 -0.06 0">
      <EventDynamicTransform baseevent="true">
        <TransformBase>
          <Ref USE="Head"/>
        </TransformBase>
        <Ref USE="lambda"/>
      </EventDynamicTransform>
    </EventTransform>
  </StbSink>

  <StbSink station="6">
    <Ref USE="lambda"/>
  </StbSink>

  <!-- PIP pointer -->

  <StbSink event="on" station="3">
    <Merge>
      <MergeDefault>
        <EventDynamicTransform baseevent="true">
          <TransformBase>
            <Ref USE="pip"/>
          </TransformBase>
          <EventTransform DEF="pointer" rotation="0 1 0 0" scale="-0.1 -0.1 0" translation="-0.05 0.05 0.001">
            <StbMouseSource DEF="TouchPad" mode="relative"/>
          </EventTransform>
        </EventDynamicTransform>
      </MergeDefault>
      <MergeButton>
        <ButtonOp DEF="Buttons" op="OR">
          <Arg1>
            <Ref USE="TouchPad"/>
          </Arg1>
          <Arg2>
            <StbKeyboardSource number="0"/>
          </Arg2>
        </ButtonOp>
      </MergeButton>
    </Merge>
  </StbSink>

  <StbSink station="7">
    <Ref USE="pointer"/>
  </StbSink>

  <StbSink station="4"> <!-- before: station 2 -->
    <TestSource frequency="1" position="0 0 0"/>
  </StbSink>

  <!-- 
    use this to add a parallel button 
  <ButtonFilter buttonmap="23016745">
    <ParButtonSource dev="0x378"/>
  </ButtonFilter>
