<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE OpenTracker SYSTEM "../../../workspace/opentracker.dtd">
<OpenTracker xmlns="http://www.studierstube.org/opentracker">
   <configuration>
    <TimeConfig rate="40"/>
   </configuration>
   <StbSink station="0">
      <StbKeyboardSource number="0"/>
   </StbSink>
   <StbSink station="1" event="on">
      <StbKeyboardSource number="1"/>
   </StbSink>
   <!-- this station is transformed by default to have the PIP in a nice place -->
   <StbSink station="2" event="on">
     <EventDynamicTransform baseevent="true">
        <StbKeyboardSource number="2"/>
        <TransformBase>
            <TestSource frequency="1000" position="0 -0.2 -0.01"/>
        </TransformBase>
     </EventDynamicTransform>
   </StbSink>
   <StbSink station="3" event="on">
      <StbKeyboardSource number="3"/>
   </StbSink>
   <StbSink station="4" event="on">
      <StbKeyboardSource number="4"/>
   </StbSink>
   <StbSink station="5" event="on">
      <StbKeyboardSource number="5"/>
   </StbSink>
   <StbSink station="6" event="on">
      <StbKeyboardSource number="6"/>
   </StbSink>
   <StbSink station="7" event="on">
      <StbKeyboardSource number="7"/>
   </StbSink>
   <StbSink station="8" event="on">
      <StbKeyboardSource number="8"/>
   </StbSink>
   <StbSink station="9" event="on">
      <StbKeyboardSource number="9"/>
   </StbSink>
   <StbSink station="10" event="on">
     <EventTransform translation="-0.5 0.5 0" scale="1 -1 1">
        <StbMouseSource mode="relative" />
     </EventTransform>
   </StbSink>
</OpenTracker>