#Inventor V2.1 ascii

SoUserKit {
   userID 1
   display SoDisplayKit {
      stereoCameraKit  File { name "../firefly_4mm_Camera.iv"}
      cameraControl SoTrackedDisplayControlMode {}
      station 1
      headlight TRUE
      headlightIntensity 1.0
      backgroundColor 0 0 0
      transparencyType DELAYED_BLEND
      videoBackground SoVideoBackgroundH323 { 
         ipAddress "128.131.167.169" # use IP address of partner machine
         audioOn TRUE
      }
   }
}

SoUserKit {
   userID 2
   display SoDisplayKit {
      stereoCameraKit  File { name "../firefly_4mm_Camera.iv"}
      cameraControl SoTrackedDisplayControlMode {}
      station 1
      xoffset 640
      headlight TRUE
      headlightIntensity 1.0
      backgroundColor 0 0 0
      transparencyType DELAYED_BLEND
      videoBackground SoVideoBackgroundH323 { audioOn TRUE }
   }
