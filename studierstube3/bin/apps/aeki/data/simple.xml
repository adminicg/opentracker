<?xml version="1.0" encoding="UTF-8"?>
<furniture xmlns="http://www.expersite.com/AEKI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.expersite.com/AEKI ../apps/aeki/data/aeki.xsd">
  <!-- Einfache Box mit 4 Connectoren -->
  <component>
    <shape>
      <cube/>
    </shape>
    <dimension unit="mm">
      <width>200</width>
      <height>200</height>
      <depth>100</depth>
    </dimension>
    <connector name="connector1" unit="mm">
      <x>10</x>
      <y>10</y>
      <z>0</z>
    </connector>
    <connector name="connector2" unit="mm">
      <x>10</x>
      <y>190</y>
      <z>0</z>
    </connector>
    <connector name="connector3" unit="mm">
      <x>190</x>
      <y>10</y>
      <z>0</z>
    </connector>
    <connector name="connector4" unit="mm">
      <x>190</x>
      <y>190</y>
      <z>0</z>
    </connector>
    <instance name="instance1"/>
    <instance name="instance2"/>
  </component>
  <!-- Bauanleitung -->
  <assembly>
    <add-component name="instance1"/>
    <add-component name="instance2">
      <connect>
        <connector-ref name="connector4"/>
        <connector-ref name="connector2" componentRef="instance1"/>
      </connect>
      <connect>
        <connector-ref name="connector1"/>
        <connector-ref name="connector3" componentRef="instance1"/>
      </connect>
      <connect>
        <connector-ref name="connector3"/>
        <connector-ref name="connector1" componentRef="instance1"/>
      </connect>
      <connect>
        <connector-ref name="connector2"/>
        <connector-ref name="connector4" componentRef="instance1"/>
      </connect>
    </add-component>
  </assembly>
</furniture>
