      pressed 0
      width 5 depth 5 height 2
      textureSwitch Switch { 
        whichChild 0 = USE VIEW_BUTTON . pressed 
        Group {
          Translation { translation 0 1 0 }
          DEF VIEW_TXT Texture2 { filename "view.gif" }
          Cube { width 4 height 0.1 depth 4 }
        }
        Group {
          USE VIEW_TXT
