#!/usr/bin/avs -network
version 5.01 (50.53 sg2)
module "volume bounds.user.0" -alias "volume bounds0" -xy 548,262 -ex $Path/avs_library/mongo
module isosurface.user.1 -alias isosurface1 -xy 388,262 -ex $Path/avs_library/tile
module "read field.user.2" -alias "read field2" -xy 388,122 -ex $Path/avs_library/mongo
module "color range.user.4" -alias "color range4" -xy 158,192 -ex $Path/avs_library/mongo
module "generate colormap.user.5" -alias "generate colormap5" -xy 148,122
module "geometry viewer.user.6" -xy 688,402
module stbWin.user.7 -xy 58,22 -ex /cg/disks/kagran/people/peci/DA/AVS/stbWin
module stbGeom.user.11 -xy 378,482 -ex /cg/disks/kagran/people/peci/DA/AVS/stbGeom
module stbGeom.user.12 -xy 548,492 -ex /cg/disks/kagran/people/peci/DA/AVS/stbGeom
port_connect "volume bounds0":0 "geometry viewer.user.6":0
port_connect "volume bounds0":0 stbGeom.user.12:2
port_connect isosurface1:0 stbGeom.user.11:2
port_connect isosurface1:0 "geometry viewer.user.6":0
port_connect "read field2":0 "volume bounds0":0
port_connect "read field2":0 isosurface1:0
port_connect "read field2":0 "color range4":0
port_connect "read field2":0 isosurface1:1
port_connect "color range4":0 isosurface1:2
port_connect "generate colormap5":0 "color range4":1
port_connect stbWin.user.7:0 stbGeom.user.12:0
port_connect stbWin.user.7:0 stbGeom.user.11:0
parm_set isosurface1:level 1.265199304 -range 0.1925999969 4.977499962
parm_set isosurface1:"optimize surf" true
parm_set isosurface1:"optimize wire" true
parm_set "read field2":"Read Field Browser" $Path/data/field/curv.fld
parm_set stbWin.user.7:"GeomSize x" 30
parm_set stbWin.user.7:"GeomSize y" 30
parm_set stbWin.user.7:"GeomSize z" 30
geom_set_scene -scene "geometry viewer.user.6"
geom_set_camera_name "Camera 1"
geom_resize_camera -view "Camera 1" 512 512
#
# State for view: Camera 1
#
geom_set_position -view "Camera 1" 0 0 -12
geom_set_view_modes -depth_cue 0 -view "Camera 1"
geom_set_view_modes -polygonal_spheres 0 -view "Camera 1"
geom_set_view_modes -stereo 0 -view "Camera 1"
geom_set_view_modes -head_tracking 1 -view "Camera 1"
geom_set_view_modes -z_buffer 1 -view "Camera 1"
geom_set_camera_params -view "Camera 1" -front -88 -back 112
geom_set_depth_cue_params "Camera 1" -scale 0.1
#
# Light state
#
geom_set_light -light 1 -type directional -state 1
geom_set_light -type ambient -state 1
#
# State for object: top
#
geom_set_cur_cli_obj top
geom_set_matrix   -mat \
                  -0.00620427    0.437152    0.110309    0 \
                    -0.0429821    -0.11039    0.435056    0 \
                    0.448802    -0.00452774    0.0431921    0 \
                    -1.08525    -0.958299    -2.29616    1 
geom_set_obj_window -7.81575 14.3622 0 8.32756 0 5.72425
#
# State for object: isosurface
#
geom_set_cur_cli_obj -push
geom_set_name_context isosurface.user.1
geom_create_obj isosurface -mod isosurface.user.1
geom_set_trans_mode parent
geom_set_obj_window -7.81575 14.3622 0 8.32756 0 5.72425
geom_set_name_context
geom_set_cur_cli_obj -pop
#
# State for object: slice
#
geom_set_cur_cli_obj -push
geom_set_name_context "arbitrary slicer.user.3"
geom_create_obj slice -mod "arbitrary slicer.user.3"
geom_set_trans_mode redirect
geom_set_render_mode no_light
geom_set_name_context
geom_set_cur_cli_obj -pop
#
# State for object: bounds
#
geom_set_cur_cli_obj -push
geom_set_name_context "volume bounds.user.0"
geom_create_obj bounds -mod "volume bounds.user.0"
geom_set_trans_mode parent
geom_set_obj_window -7.81575 14.3622 0 8.32756 0 5.72425
geom_set_name_context
geom_set_cur_cli_obj -pop
shell "ui" shell
 panel Application -w app_panel -p ui -xy 0,0 -wh 260,984
  panel "Top Level Stack" -w master_stack -p Application -xy 2,100 -wh 256,762
   panel "volume bounds0" -w panel -p "Top Level Stack" -xy 0,64 -wh 138,174
    manipulator "volume bounds0:Hull" -w toggle -p "volume bounds0" \
        -xy 10,10 -wh 118,22
    manipulator "volume bounds0:Min I" -w toggle -p "volume bounds0" \
        -xy 10,32 -wh 118,22
    manipulator "volume bounds0:Max I" -w toggle -p "volume bounds0" \
        -xy 10,54 -wh 118,22
    manipulator "volume bounds0:Min J" -w toggle -p "volume bounds0" \
        -xy 10,76 -wh 118,22
    manipulator "volume bounds0:Max J" -w toggle -p "volume bounds0" \
        -xy 10,98 -wh 118,22
    manipulator "volume bounds0:Min K" -w toggle -p "volume bounds0" \
        -xy 10,120 -wh 118,22
    manipulator "volume bounds0:Max K" -w toggle -p "volume bounds0" \
        -xy 10,142 -wh 118,22
    manipulator "volume bounds0:Colored Bounds" -w toggle -p "volume bounds0" \
        -xy 10,10 -wh 118,22
   panel isosurface1 -w panel -p "Top Level Stack" -xy 0,64 -wh 228,194
    manipulator isosurface1:level -w dial -p isosurface1 -xy 10,10 -wh 90,130\
   -P local_range real 4.9
    manipulator "isosurface1:optimize surf" -w toggle -p isosurface1 \
        -xy 100,10 -wh 118,22
    manipulator "isosurface1:optimize wire" -w toggle -p isosurface1 \
        -xy 10,140 -wh 118,22
    manipulator "isosurface1:flip normals" -w toggle -p isosurface1 \
        -xy 10,162 -wh 118,22
    manipulator "isosurface1:generate normals" -w toggle -p isosurface1 \
        -xy 10,10 -wh 118,22
   panel "read field2" -w panel -p "Top Level Stack" -xy 0,64 -wh 256,280
    manipulator "read field2:Read Field Browser" -w browser -p "read field2" \
        -xy 10,10 -wh 236,194
    manipulator "read field2:Read Status" -w textblock -p "read field2" \
        -xy 10,204 -wh 236,66
    manipulator "read field2:Data Conversion" -w radio_buttons -p "read field2" \
        -xy 10,10 -wh 118,44
   panel "generate colormap5" -w panel -p "Top Level Stack" -xy 0,64 -wh 256,566
    manipulator "generate colormap5:colormap" -w color_editor -p "generate colormap5" \
        -xy 10,10 -wh 236,416
    manipulator "generate colormap5:lo value" -w dial -p "generate colormap5" \
        -xy 10,426 -wh 90,130
    manipulator "generate colormap5:hi value" -w dial -p "generate colormap5" \
        -xy 100,426 -wh 90,130
   panel stbWin.user.7 -w panel -p "Top Level Stack" -xy 0,64 -wh 256,456
    manipulator stbWin.user.7:Host: -w typein -p stbWin.user.7 -hide \
        -xy 10,10 -wh 118,22
    manipulator stbWin.user.7:Host -w text -p stbWin.user.7 -xy 10,10 -wh 118,22
    manipulator stbWin.user.7:Port: -w typein -p stbWin.user.7 -hide \
        -xy 10,32 -wh 118,22
    manipulator stbWin.user.7:Port -w text -p stbWin.user.7 -xy 10,30 -wh 118,22
    manipulator stbWin.user.7:connect -w toggle -p stbWin.user.7 \
        -xy 10,50 -wh 118,22
    manipulator "stbWin.user.7:Pos x" -w typein_real -p stbWin.user.7 \
        -xy 10,72 -wh 118,22
    manipulator "stbWin.user.7:Pos y" -w typein_real -p stbWin.user.7 \
        -xy 10,94 -wh 118,22
    manipulator "stbWin.user.7:Pos z" -w typein_real -p stbWin.user.7 \
        -xy 10,116 -wh 118,22
    manipulator "stbWin.user.7:Rot x" -w typein_real -p stbWin.user.7 \
        -xy 10,138 -wh 118,22
    manipulator "stbWin.user.7:Rot y" -w typein_real -p stbWin.user.7 \
        -xy 10,160 -wh 118,22
    manipulator "stbWin.user.7:Rot z" -w typein_real -p stbWin.user.7 \
        -xy 10,182 -wh 118,22
    manipulator "stbWin.user.7:Rot w" -w typein_real -p stbWin.user.7 \
        -xy 10,204 -wh 118,22
    manipulator "stbWin.user.7:Size x" -w typein_real -p stbWin.user.7 \
        -xy 10,226 -wh 118,22
    manipulator "stbWin.user.7:Size y" -w typein_real -p stbWin.user.7 \
        -xy 10,248 -wh 118,22
    manipulator "stbWin.user.7:Size z" -w typein_real -p stbWin.user.7 \
        -xy 10,270 -wh 118,22
    manipulator "stbWin.user.7:GeomSize x" -w typein_real -p stbWin.user.7 \
        -xy 10,292 -wh 236,22
    manipulator "stbWin.user.7:GeomSize y" -w typein_real -p stbWin.user.7 \
        -xy 10,314 -wh 236,22
    manipulator "stbWin.user.7:GeomSize z" -w typein_real -p stbWin.user.7 \
        -xy 10,336 -wh 236,22
    manipulator "stbWin.user.7:GeomMove x" -w typein_real -p stbWin.user.7 \
        -xy 10,358 -wh 236,22
    manipulator "stbWin.user.7:GeomMove y" -w typein_real -p stbWin.user.7 \
        -xy 10,380 -wh 236,22
    manipulator "stbWin.user.7:GeomMove z" -w typein_real -p stbWin.user.7 \
        -xy 10,402 -wh 236,22
    manipulator "stbWin.user.7:fit Geometry" -w oneshot -p stbWin.user.7 \
        -xy 10,424 -wh 118,22
   panel stbGeom.user.11 -w panel -p "Top Level Stack" -xy 0,64 -wh 138,64
    manipulator "stbGeom.user.11:clear Geometry" -w radio_buttons -p stbGeom.user.11 \
        -xy 10,10 -wh 118,44
   panel stbGeom.user.12 -w panel -p "Top Level Stack" -xy 0,196 -wh 138,64
    manipulator "stbGeom.user.12:clear Geometry" -w radio_buttons -p stbGeom.user.12 \
        -xy 10,10 -wh 118,44
 panel "geometry viewer.user.6!display" -w container -p ui -xy 701,148 -wh 552,584\
   -P zoom_coords string "0 0 0 0 0 <$NULL> 0 0 0 0"
manipulator "geometry viewer.user.6":object -w none
manipulator "geometry viewer.user.6":"Update Always" -w none
manipulator "geometry viewer.user.6":"Update Image" -w none
# End of file
