<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE OpenTracker SYSTEM "../../studierstube/bin/workspace/opentracker.dtd">
<OpenTracker>
	<configuration>
		<ARToolKitConfig camera-parameter="..\..\studierstube\bin\workspace\data\webcam.dat" videomode="showDlg" framerate="10" treshhold="100"/>
		<ConsoleConfig headerline="Sample Tracking Input" interval="20"/>
	</configuration>
	<StbSink station="0">
		<ConsoleSink comment="Pen">
			<StbKeyboardSource number="0"/>
		</ConsoleSink>
	</StbSink>
	<StbSink station="1">
		<ConsoleSink comment="Pip">
			<StbKeyboardSource number="1"/>
		</ConsoleSink>
	</StbSink>
	<StbSink station="4" event="on">
		<ConsoleSink comment="Cube Green Side">
			<EventTransform scale="0.001 0.001 0.001" rotation="1 0 0 0" translation="0 0 0">
				<ARToolKitSource center="0 0" size="50 50" tag-file="..\..\studierstube\bin\workspace\data\pi.patt"/>
			</EventTransform>
		</ConsoleSink>
	</StbSink>
	<StbSink station="5" event="on">
		<ConsoleSink comment="Cube Blue Side">
			<EventTransform scale="0.001 0.001 0.001" rotation="1 0 0 0" translation="0 0 0">
				<ARToolKitSource center="0 0" size="50 50" tag-file="..\..\studierstube\bin\workspace\data\kappa.patt"/>
			</EventTransform>
		</ConsoleSink>
	</StbSink>
	<StbSink station="6">
		<ConsoleSink comment="Cylinder">
			<EventTransform scale="0.001 0.001 0.001" rotation="1 0 0 0" translation="0 0 0">
				<ARToolKitSource center="0 0" size="50 50" tag-file="..\..\studierstube\bin\workspace\data\lambda.patt"/>
			</EventTransform>
		</ConsoleSink>
	</StbSink>
</OpenTracker>
