<!--simple test configuration for the new tracker serve system-->
<!DOCTYPE OpenTracker SYSTEM "opentracker.dtd">
<OpenTracker>
	<configuration>
		<NetworkSinkConfig name="Test Server"/>
		<ConsoleConfig interval="20">
			<KeyDefinition key="left" name="Move_X_minus"/>
		</ConsoleConfig>
	</configuration>
	<Merge>
		<MergePosition>
			<ConsoleSink>
				<Merge>
					<MergeDefault>
						<ARToolKitSource tag-file="" vertex=""/>
					</MergeDefault>
					<MergeOrientation>
						<TestSource/>
					</MergeOrientation>
					<MergeConfidence>
						<NetworkSource number="" multicast-address="" port=""/>
					</MergeConfidence>
				</Merge>
			</ConsoleSink>
			<NetworkSink name="" number="" multicast-address="" port=""/>
		</MergePosition>
		<MergeButton>
			<ConsoleSink>
				<EventDynamicTransformation>
					<TestSource/>
					<TransformBase>
						<InterTraxSource/>
					</TransformBase>
				</EventDynamicTransformation>
			</ConsoleSink>
		</MergeButton>
	</Merge>
</OpenTracker>
